{
  "extends": "airbnb/base",
  "plugins": [
      "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {  
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true
  }
}